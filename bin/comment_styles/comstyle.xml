<profile>
	<!--Link to target style sheet. Multiple may be declared, loaded in given order-->
	<styles>
		<style>stdStyle.css</style>
	</styles>

	<langauges>
		<language>	C		</language>
		<language>	Cpp		</language>
		<language>	Java	</language>
		<language>	C#		</language>
		<language>	Python	</language>
	</langauges>
	
	<!--Will be applied to all profiles unless <Override/> tag added to property-->
	<Default>
		<SingleComment>
			<start> "///" </start>
		<SingleComment>
		<BlockComment>
			<start> "/**" </start>
			<end> "*/" </start>
		</BlockComment>
	</Default>
	
	<C>
		<dependency>
			<verb>"#include"</verb>
			<closures>
				<closure>
					<start> "<" </start>
					<end> ">" </end>
				</closure>
				<closure>
					<start> "\"" </start>
					<end> "\"" </end>
				</closure>
			</closures>
		</dependency>
	</C>
	
	

</profile>